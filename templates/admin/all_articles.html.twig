<h1>Les Articles</h1>
{% for article in articles %}
    <li>{{ article.title }}
        <a href="{{ path('showArticle', {'slug': article.slug}) }}">Afficher</a>
        <a href="{{ path('updateArticle', {'slug': article.slug}) }}">Modifier</a>
        <a href="{{ path('deleteArticle', {'slug': article.slug}) }}">Supprimer</a>
    </li>

{% endfor %}