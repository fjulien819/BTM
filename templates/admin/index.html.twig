{{ dump(pages) }}
{% for page in pages %}
    <li>{{ page.title }}</li>
    <a href="{{ path('showPage', {'slug': page.slug}) }}">Afficher</a>
    <a href="{{ path('updatePage', {'slug': page.slug}) }}">Modifier</a>
    <a href="{{ path('deletePage', {'slug': page.slug}) }}">Supprimer</a>
{% endfor %}

