{% extends 'base_blog.html.twig' %}

{% block title %}
    {{ parent() }}{{ post.title }} 
{% endblock %}

{% block metaDescription %}
    {{ post.metaDescription }}
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
{% endblock %}

{% block header %}
    {{ parent() }}
{% endblock %}

{% block body %}
    {{ parent() }}
{% endblock %}

    {% block blog_content %}
        <div class="row p-0 col-12 d-flex  mx-auto m-0 d-flex">
            <div class="p-0 col-12">
                <h1>Le blog Back To Mobile</h1>
                <!-- Search Widget -->
                <div class="card mb-3 d-flex d-lg-none rounded-0">
                    <div class="card-body postbody p-0">
                        <!-- Research form -->
                        {% include "form/_search_form.html.twig" %}
                    </div>
                </div>
            </div>
            {% include "_show_post.html.twig" %}
            {% include "_sidebar.html.twig" %}
        </div>
    {% endblock %}

{% block footer %}
    {{ parent() }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
{% endblock %}





